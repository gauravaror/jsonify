{
  "manifest_version": 2,

  "name": "Jsonify",
  "description": "Give the json representation of current page",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],
     "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
